<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="returnInstanceForEmptyRow" value="true"/>
	</settings>
	

	<mappers>
		<mapper resource="config/mybatis/mapper/mainpage/board.xml" />
		<mapper resource="config/mybatis/mapper/mainpage/moveMemo.xml" />
		<mapper resource="config/mybatis/mapper/mainpage/chatting.xml" />
		
		<mapper resource="config/mybatis/mapper/loginpage/logIn.xml" />
		<mapper resource="config/mybatis/mapper/mypage/mypage.xml" />
	</mappers>
	
</configuration>


